@namespace Langueedu.Components

<form class="form-inline search-form" action="#" method="get">
    <div class="form-group">
        <div class="Typeahead Typeahead--twitterUsers">
            <div class="u-posRelative">
                <input class="Typeahead-input form-control-plaintext" id="demo-input" type="text" name="q"
                    placeholder="Search Artist, Album, Song..." />
                <div class="spinner-border Typeahead-spinner d-none" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
                <span class="d-sm-none mobile-search">

                    <i class="fa-solid fa-magnifying-glass"></i>

                </span>
            </div>
            <div class="Typeahead-menu"></div>
        </div>
    </div>
</form>